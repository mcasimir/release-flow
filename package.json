{
  "name": "release-flow",
  "version": "1.2.2",
  "description": "Git flow conventional releases",
  "main": "dist/Release.js",
  "engines": {
    "node": ">=12",
    "npm": ">=8"
  },
  "scripts": {
    "test": "mocha -r ts-node/register 'test/**/*.spec.js'",
    "lint": "eslint . --quiet",
    "clean": "rm -Rf ./lib",
    "build": "npm run clean && babel src -d lib",
    "release": "node ./lib/cli.js",
    "compile": "tsc -p tsconfig.json"
  },
  "pre-commit": {
    "run": [
      "build"
    ]
  },
  "bin": {
    "release-flow": "./lib/cli.js"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "mcasimir (https://github.com/mcasimir)",
  "repository": "mcasimir/release-flow",
  "license": "ISC",
  "dependencies": {
    "chalk": "1.1.3",
    "conventional-commits-filter": "1.0.0",
    "conventional-commits-parser": "1.2.2",
    "memoize-decorator": "1.0.2",
    "prepend-file": "1.3.0",
    "semver": "5.3.0",
    "yargs": "4.8.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "eslint": "3.2.0",
    "eslint-config-google": "0.6.0",
    "eslint-plugin-babel": "3.3.0",
    "mocha": "^10.0.0",
    "mock-require": "1.3.0",
    "nyc": "^15.1.0",
    "release-it": "^15.4.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "release-it": {}
}
